# Suez Tout Sur Mon Eau credentials
SUEZ_EMAIL=your.email@example.com
SUEZ_PASSWORD=your_password
SUEZ_ID_PDS=your_id_pds

# SSL Verification (set to false if you have certificate issues, e.g., behind corporate proxy)
# WARNING: Disabling SSL verification reduces security
VERIFY_SSL=true

# MQTT Configuration
MQTT_BROKER=localhost
MQTT_PORT=1883
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_TOPIC=water

# Heartbeat Configuration (optional)
# Interval in seconds for publishing heartbeat messages to {MQTT_TOPIC}/heartbeat
HEARTBEAT_INTERVAL=60
